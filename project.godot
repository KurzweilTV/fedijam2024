config_version=5

[application]

config/name="FediJam"
config/version="0.1"
config/tags=PackedStringArray("gamejam")
run/main_scene="res://ui/preload/preloader.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://globals/GameManager.gd"
Player="*res://globals/Player.gd"
Messages="*res://globals/Messages.gd"
ParticleCache="*res://globals/ParticleCache.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1200
window/size/viewport_height=800
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/jamcountdown/plugin.cfg")

[file_customization]

folder_colors={
"res://actors/player/": "blue",
"res://ui/": "green",
"res://ui/player/": "blue"
}

[global_group]

player=""
spawn=""

[gui]

theme/custom="res://ui/themes/cheerful.tres"
theme/custom_font="res://assets/fonts/Kenney/Kenney Future.ttf"

